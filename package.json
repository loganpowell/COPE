{
  "name": "ts-starter",
  "author": "Logan Powell",
  "license": "MIT",
  "version": "0.2.67",
  "description": "typescript configuration and starter project",
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/loganpowell/ts-starter",
  "homepage": "https://github.com/loganpowell/ts-starter",
  "scripts": {
    "clean": "rm -rf ./lib",
    "typewatch": "tsc --project tsconfig.json",
    "types": "tsc --project tsconfig.build.json",
    "madge": "madge --circular lib/",
    "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
    "pages": "gh-pages -d @-0/spool",
    "docs": "npm run types && npm run jsdoc",
    "patch": "npm version patch && npm run docs && npm run pages && npm publish",
    "tests": "concurrently \"npm run typewatch\" \"npm run madge && jest --watchAll\"",
    "postgit": "git push origin master && npm run patch",
    "ncu": "ncu -u && npm i && npm audit fix",
    "git": "npm run ncu && git add . && git commit -m"
  },
  "keywords": [
    "typescript",
    "starter"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^25.3.0",
    "better-docs": "^1.4.7",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "endOfLine": "lf",
    "tabWidth": 2,
    "semi": false,
    "arrowParens": "avoid",
    "bracketSpacing": true
  }
}
