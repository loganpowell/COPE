{
    "name": "COPE",
    "author": "Logan Powell",
    "license": "MIT",
    "version": "0.0.1",
    "description": "Create Once Publish Everywhere",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "repository": "https://github.com/loganpowell/COPE",
    "homepage": "https://github.com/loganpowell/COPE",
    "scripts": {
        "clean": "rm -rf ./lib & rm -rf ./dist &  rm -rf .cache",
        "cleanFnConfirm": "rm -rf ./amplify/backend/function/COPECognitoPostConfirmTriggerAddToViewers/src",
        "cpFnConfirm": "cp -r ./function/confirmed/src ./amplify/backend/function/COPECognitoPostConfirmTriggerAddToViewers",
        "bndFnConfirm": "npm run cleanFnConfirm && npm run cpFnConfirm",
        "cleanFnEdge": "rm -rf ./amplify/backend/function/CopeEdgeCRUDLambda/src",
        "cpFnEdge": "cp -r ./function/edge/src ./amplify/backend/function/CopeEdgeCRUDLambda",
        "bundleFnEdge": "npm run cleanFnEdge && npm run cpFnEdge",
        "madge": "madge --circular lib/",
        "typewatch": "tsc --project tsconfig.json",
        "types": "tsc --project tsconfig.build.json",
        "mock": "amplify mock",
        "jest": "jest --watchAll",
        "delay": "node ./node_modules/npm-delay 60000",
        "test": "npm run madge && concurrently \"amplify mock\" \"npm run delay && npm run typewatch\" \"npm run delay && npm run jest\"",
        "git": "npm run ncu && git add . && git commit -m"
    },
    "keywords": [],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@aws-amplify/auth": "^4.3.18",
        "@aws-amplify/core": "^4.3.10",
        "@aws-amplify/storage": "^4.4.11",
        "@aws-sdk/client-ssm": "^3.44.0"
    },
    "devDependencies": {
        "npm-delay": "^1.0.4",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "browserslist": [
        "last 3 Chrome versions"
    ],
    "prettier": {
        "singleQuote": false,
        "printWidth": 100,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid",
        "useTabs": false,
        "trailingComma": "all",
        "bracketSpacing": true
    }
}
